{
  "name": "Chrome Extension Offline",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "谷歌离线插件",
  "author": "lijh <xff777qq.com>",
  "homepage_url": "https://github.com/keepAwayFrom404",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "tabs",
    "<all_urls>",
    "background",
    "storage",
    "alarms",
    "webNavigation",
    "notifications"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "scripts/content.js"
      ],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_icon": {
      "19": "icons/16.png",
      "38": "icons/32.png"
    },
    "default_title": "Chrome Extension Offline",
    "default_popup": "popup.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "web_accessible_resources": []
}